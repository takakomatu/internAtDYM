/* Main Visual
   ========================================================================== */
/* 装飾 */
#main_visual .bx-controls-direction,
#main_visual .bx-pager {
	text-align:center;
	height:25px;
}
#main_visual .bx-pager {
	display:block;
	width:275px;
	margin:0 auto;
	text-align:right;
}
#main_visual .bx-controls-direction a,
#main_visual .bx-pager-item {
	display:inline-block;
	text-align:center;
}
#main_visual .bx-pager-item a{
	display:block;
	box-sizing:border-box;
	margin:20px 5px 0px;
	width:30px;
	height:9px;
	text-indent:-9999px;
	background:#656a87;
	border:1px solid #656a87;
	border-radius:0px;
}
#main_visual .bx-pager-item a.active{
	opacity:1.0;
	border:1px solid #FFF;
	background:#1e2c5b;
}
#main_visual .bx-controls-direction a{
	text-decoration:none;
	color:#000;
	padding:0 5px;
}
#main_visual .bx-controls{
	position:absolute;
	bottom:5px;
	z-index:100;
	width:100%;
}
#main_visual .bx-viewport{
	height: 500px !important;
}
#main_visual .bx-wrapper .bx-controls-direction a {
	display:none;
}




/* recommend
   ========================================================================== */
/*** layout ***/
#recommend .recommend_inner .bx-wrapper{
	position: relative;
	margin: 0 auto 0px;
	padding: 0;
	*zoom: 1;
}
#recommend .recommend_inner .bx-wrapper img {
	max-width: 100%;
	display: block;
}

/*** pager ***/
#recommend .recommend_inner .bx-wrapper .bx-pager {
	text-align: center;
	font-size: .85em;
	font-family: Arial;
	font-weight: bold;
	color: #666;
	padding-top: 20px;
}
#recommend .recommend_inner .bx-wrapper .bx-pager .bx-pager-item,
#recommend .recommend_inner .bx-wrapper .bx-controls-auto .bx-controls-auto-item {
	display: inline-block;
	*zoom: 1;
	*display: inline;
}
#recommend .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a {
	background: #1e2c5c;
	text-indent: -9999px;
	display: block;
	width: 10px;
	height: 10px;
	margin: 0 12px;
	outline: 0;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border:1px solid #ece0ba;
}
#recommend .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a:hover,
#recommend .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a.active {
	background: #ece0ba;
	border:1px solid #ece0ba;
}

#recommend .recommend_inner .bx-wrapper .bx-controls-direction a {
	display:none;
}


/* recommend
   ========================================================================== */
/*** layout ***/
#recommend_vertical .recommend_inner .bx-wrapper{
	position: relative;
	margin: 0px auto 0px;
	padding: 0;
}
#recommend_vertical .recommend_inner .bx-wrapper img {
	max-width: 100%;
	display: block;
}

/*** pager ***/
#recommend_vertical .recommend_inner .bx-wrapper .bx-pager {
	text-align: center;
	font-size: .85em;
	font-family: Arial;
	font-weight: bold;
	color: #666;
	padding-top: 20px;
}
#recommend_vertical .recommend_inner .bx-wrapper .bx-pager .bx-pager-item,
#recommend_vertical .recommend_inner .bx-wrapper .bx-controls-auto .bx-controls-auto-item {
	display: inline-block;
}
#recommend_vertical .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a {
	background: #1e2c5c;
	text-indent: -9999px;
	display: block;
	width: 10px;
	height: 10px;
	margin: 0 12px;
	outline: 0;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border:1px solid #ece0ba;
}
#recommend_vertical .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a:hover,
#recommend_vertical .recommend_inner .bx-wrapper .bx-pager.bx-default-pager a.active {
	background: #ece0ba;
	border:1px solid #ece0ba;
}


#recommend_vertical .recommend_inner .bx-wrapper .bx-prev {
	position:absolute;
	bottom:15px;
	left:40px;
    width: 20px;
    height: 20px;
    border-top: 5px solid #1e2b5f;
    border-right: 5px solid #1e2b5f;
    -webkit-transform: rotate(135deg);
    transform: rotate(135deg);
}

#recommend_vertical .recommend_inner .bx-wrapper .bx-next {
	position:absolute;
	top:-700px;
	left:40px;
    width: 20px;
    height: 20px;
    border-top: 5px solid #1e2b5f;
    border-right: 5px solid #1e2b5f;
    -webkit-transform: rotate(315deg);
    transform: rotate(315deg);
}
#recommend_vertical .recommend_inner .bx-wrapper .bx-controls-direction{
	position: relative;
}
#recommend_vertical .recommend_inner .bx-wrapper .bx-controls-direction a {
	display: block;
	text-indent: -9999px;
}

#recommend_vertical .bx-viewport{
	margin-top: 50px;
	margin-bottom: 50px;
}
