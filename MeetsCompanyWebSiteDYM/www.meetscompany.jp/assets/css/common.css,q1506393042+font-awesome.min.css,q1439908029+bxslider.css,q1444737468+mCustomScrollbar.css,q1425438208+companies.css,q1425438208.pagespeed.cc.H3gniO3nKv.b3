/* 注目企業(縦スクロール)
   ========================================================================== */
#recommend_vertical{
	margin:40px 0px;
}
#recommend_vertical h2{
	position:relative;
	display:block;
	padding:15px 0px;
	width:200px;
	background:#1e2c5b;
	color:#fff;
	text-align:center;
	font-weight:bold;
}
#recommend_vertical h2:before{
	content: "";
	display: block;
	position: absolute;
	left: 50%;
