/* ページタイトル
   ========================================================================== */
#main_head{
	background:#1e2c5c;
        min-width:980px;
}
#main_head h2{
	padding:15px 0px;
	margin:0px auto;
	width:980px;
	text-align:center;
	font-size:34px;
	font-weight:normal;
	color:#FFFFFF;
}
#main_head h2 span{
	display:block;
	font-size:16px;
	font-weight:normal;
	font-family: 'Carrois Gothic SC',"arial", sans-serif;
}

/* topicpath */
#main_head nav#topicpath{
	background:#32468b;
}
#main_head nav#topicpath ol{
	margin:0px auto;
	padding:2px 0px;
	width:980px;
	overflow:hidden;
	color:#9bace4;
	box-sizing:border-box;
}
#main_head nav#topicpath ol li{
	display: inline-block;
	font-size:12px;
}

#main_head nav#topicpath ol li:first-child a span {
    margin: 0 5px 0 0;
}
#main_head nav#topicpath ol li:after {
	padding:0px 8px;
	content: "»";
}
#main_head nav#topicpath ol li:last-child:after {
	content: "";
}
#main_head nav#topicpath ol li.home img{
	vertical-align:middle;
	margin-bottom:4px;
}
#main_head nav#topicpath ol li a{
	color:#9bace4;
	text-decoration:none;
}



/* メインコンテンツ枠
   ========================================================================== */
#main_contents{
	margin: 30px auto;
	width:980px;
	box-sizing:border-box;
	overflow:hidden;
}
#main_contents .content_col_left{
	float:left;
	width:200px;
	box-sizing:border-box;
	position:relative;
}
#main_contents .content_col_right{
	float:right;
	width:760px;
	box-sizing:border-box;
