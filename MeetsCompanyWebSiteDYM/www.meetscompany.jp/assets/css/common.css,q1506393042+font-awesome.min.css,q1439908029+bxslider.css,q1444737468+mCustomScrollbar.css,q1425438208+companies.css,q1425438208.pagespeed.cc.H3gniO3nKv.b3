.content_body .company_list input[type=radio],
.content_body .company_list input[type=checkbox] {
    display: inline-block;
    margin-right: 6px;
}
.content_body .company_list input[type=radio] + label,
.content_body .company_list input[type=checkbox] + label {
    position: relative;

    display: inline-block;
    margin-right: 12px;

    font-size: 14px;
    line-height: 30px;

    cursor: pointer;
}

@media (min-width: 1px) {
    .content_body .company_list input[type=radio],
    .content_body .company_list input[type=checkbox] {
        display: none;
        margin: 0;
    }
    .content_body .company_list input[type=radio] + label,
    .content_body .company_list input[type=checkbox] + label {
        padding: 0 0 0 24px;
    }
    .content_body .company_list input[type=radio] + label::before,
    .content_body .company_list input[type=checkbox] + label::before {
        content: "";
        position: absolute;
        top: 50%;
        left: 0;

        -moz-box-sizing: border-box;
        box-sizing: border-box;
        display: block;
        width: 18px;
        height: 18px;
        margin-top: -9px;

        background: #FFF;
    }
    .content_body .company_list input[type=radio] + label::before {
        border: 2px solid #ccc;
        border-radius: 30px;
    }
    .content_body .company_list input[type=checkbox] + label::before {
        border: 2px solid #ccc;
    }
    .content_body .company_list input[type=radio]:checked + label::after,
    .content_body .company_list input[type=checkbox]:checked + label::after {
        content: "";
        position: absolute;
        top: 50%;

        -moz-box-sizing: border-box;
        box-sizing: border-box;
        display: block;
    }
    .content_body .company_list input[type=radio]:checked + label::after {
        left: 5px;

        width: 8px;
        height: 8px;
        margin-top: -4px;

        background: #E71063;
        border-radius: 8px;
    }
    .content_body .company_list input[type=checkbox]:checked + label::after {
        left: 3px;

        width: 12px;
        height: 6px;
        margin-top: -5px;

        border-left: 3px solid #1E2C5C;
        border-bottom: 3px solid #1E2C5C;

        -webkit-transform: rotate(-45deg);
        -ms-transform: rotate(-45deg);
        transform: rotate(-45deg);
    }
}


